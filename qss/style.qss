QLabel#tipLabel{
    color:#0000FF;
}

QLabel#tipLabel::hover{
    color:#FF0000;
}

QWidget#deviceWidget{
    background-color:rgb(115,132,140);
}

QLabel#deviceLabel{
    background-color:rgb(115,132,140);
    color:#FFFF00;
    font-size:18px;
    alignment:center;
}

QTextBrowser#userTextBrowser{
    border:none;
}

QLabel#tipedLabel{
    color:red;
    font-size:16px;
}
